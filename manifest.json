{
  "author": "igorlogius",
  "homepage_url": "https://github.com/igorlogius/dont-load-background-tabs",
  "description": "Prevents background tabs from loading until they have been activated",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_area": "navbar"
  },
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "Dont Load Background Tabs",
  "options_ui": {
    "page": "options.html"
  },
  "permissions": ["storage", "webRequest", "webRequestBlocking", "<all_urls>"],
  "version": "1.3.20"
}
